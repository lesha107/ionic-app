{
  "name": "angular-ionic-starter",
  "version": "0.0.1",
  "author": "Viacheslav Saloid",
  "engines": {
    "npm": "please-use-yarn",
    "yarn": ">= 1.22.4"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "lint-css": "stylelint src/**/*.scss",
    "prettier": "prettier --write \"**/*.{js,json,css,scss,less,md,ts,html,component.html}\"",
    "commit": "npx git-cz",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "dev": "ng serve",
    "hmr": "ng serve --configuration hmr",
    "prod": "ng build --prod",
    "dev:ssr": "ng run angular-ionic-starter:serve-ssr",
    "serve:ssr": "node dist/angular-ionic-starter/server/main.js",
    "build:ssr": "ng build --prod && ng run angular-ionic-starter:server:production",
    "prerender": "ng run angular-ionic-starter:prerender"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.0",
    "@angular/cdk": "^10.2.7",
    "@angular/common": "~10.0.0",
    "@angular/core": "~10.0.0",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "~10.0.0",
    "@angular/localize": "10.0.14",
    "@angular/material": "^10.2.7",
    "@angular/platform-browser": "~10.0.0",
    "@angular/platform-browser-dynamic": "~10.0.0",
    "@angular/platform-server": "~10.0.0",
    "@angular/router": "~10.0.0",
    "@capacitor/core": "2.4.2",
    "@datorama/akita": "^5.2.5",
    "@datorama/akita-cli": "^6.1.1",
    "@datorama/akita-ng-entity-service": "^5.0.2",
    "@datorama/akita-ng-router-store": "^5.1.8",
    "@datorama/akita-ngdevtools": "^5.0.3",
    "@ionic-native/core": "^5.0.0",
    "@ionic-native/splash-screen": "^5.0.0",
    "@ionic-native/status-bar": "^5.0.0",
    "@ionic/angular": "^5.0.0",
    "@ngneat/hotkeys": "^1.1.1",
    "@ngneat/until-destroy": "^8.0.3",
    "@nguniversal/express-engine": "^10.0.2",
    "@schuchard/prettier": "^3.1.0",
    "angular-calendar": "^0.28.22",
    "express": "^4.15.2",
    "moment": "^2.29.1",
    "ng-animate": "^0.3.4",
    "ngx-avatar": "^4.0.0",
    "ngx-dropzone": "^2.2.2",
    "ngx-mask": "^11.0.1",
    "ngx-skeleton-loader": "^2.6.0",
    "normalize.css": "^8.0.1",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.0",
    "@angular-eslint/builder": "0.8.0-beta.1",
    "@angular-eslint/eslint-plugin": "0.8.0-beta.1",
    "@angular-eslint/eslint-plugin-template": "0.8.0-beta.1",
    "@angular-eslint/schematics": "^0.8.0-beta.1",
    "@angular-eslint/template-parser": "0.8.0-beta.1",
    "@angular/cli": "~10.0.5",
    "@angular/compiler": "~10.0.0",
    "@angular/compiler-cli": "~10.0.0",
    "@angular/language-service": "~10.0.0",
    "@angularclass/hmr": "^3.0.0",
    "@capacitor/cli": "2.4.2",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@ionic/angular-toolkit": "^2.3.0",
    "@nguniversal/builders": "^10.0.2",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "codelyzer": "^6.0.0",
    "conventional-changelog-cli": "^2.1.1",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jasmine": "^4.1.1",
    "eslint-plugin-jsdoc": "^30.7.8",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-simple-import-sort": "^6.0.0",
    "husky": "4.3.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "10.5.1",
    "prettier": "^2.1.2",
    "protractor": "~7.0.0",
    "stylelint": "^13.8.0",
    "stylelint-config-standard": "^20.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "^4.0.5"
  },
  "description": "An Angular Ionic Starter",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
